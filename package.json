{
    "name": "ninelines-template",
    "version": "1.0.0",
    "description": "Web starter kit",
    "engines": {
        "node": ">=16.13",
        "npm": ">=8.1"
    },
    "repository": "git+https://github.com/ninelines/ninelines-template.git",
    "keywords": [
        "webpack",
        "starter",
        "kit"
    ],
    "author": "Denis Kobelev | DaveDev <deniskobeleff@yandex.ru>",
    "license": "MIT",
    "homepage": "https://github.com/ninelines/ninelines-template#readme",
    "private": true,
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --mode production",
        "clean": "rm build",
        "create": "node presets.js",
        "dev": "cross-env NODE_ENV=development webpack-dashboard -- webpack serve --mode development",
        "image:optimize": "cross-env NODE_ENV=production gulp image:optimize",
        "lint:scripts": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:styles": "stylelint",
        "lint:pug": "pug-lint -r inline src",
        "lint": "concurrently --kill-others 'eslint' 'stylelint \"**/*.(css|scss)\"' 'pug-lint'",
        "prepare": "husky install",
        "pre-commit": "lint-staged",
        "start": "cross-env NODE_ENV=development webpack serve --mode development",
        "update": "yarn upgrade-interactive --latest",
        "zip": "cross-env NODE_ENV=production gulp zip"
    },
    "devDependencies": {
        "@babel/core": "^7.17.10",
        "@babel/eslint-parser": "7.17.0",
        "@babel/preset-env": "^7.17.10",
        "@babel/preset-typescript": "7.16.7",
        "@babel/register": "^7.17.7",
        "@types/jquery": "3.5.14",
        "@webdiscus/pug-loader": "2.0.0",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "7.1.0",
        "copy-webpack-plugin": "^10.2.4",
        "core-js": "3.22.4",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.15.0",
        "eslint-webpack-plugin": "3.1.1",
        "extract-loader": "^5.1.0",
        "file-loader": "^6.2.0",
        "glob": "8.0.1",
        "html-loader": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "7.0.4",
        "image-webpack-loader": "8.1.0",
        "interpolate-html-plugin": "^4.0.0",
        "lint-staged": "12.4.1",
        "mini-css-extract-plugin": "^2.6.0",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.13",
        "postcss-combine-media-query": "^1.0.1",
        "postcss-loader": "^6.2.1",
        "postcss-preset-env": "^7.5.0",
        "postcss-scss": "4.0.4",
        "pug": "^3.0.2",
        "pug-lint": "2.6.0",
        "sass": "^1.51.0",
        "sass-loader": "^12.6.0",
        "style-loader": "^3.3.1",
        "stylelint": "14.8.2",
        "stylelint-declaration-block-no-ignored-properties": "2.5.0",
        "stylelint-order": "5.0.0",
        "stylelint-scss": "4.2.0",
        "svg-spritemap-webpack-plugin": "4.3.3",
        "terser-webpack-plugin": "^5.3.1",
        "typescript": "4.6.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.72.0",
        "webpack-build-notifier": "2.3.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "webpack-dashboard": "3.3.7",
        "webpack-dev-server": "^4.9.0",
        "webpack-merge": "^5.8.0",
        "webpack-pwa-manifest": "4.3.0"
    },
    "dependencies": {
        "body-scroll-lock": "4.0.0-beta.0",
        "jquery": "3.6.0",
        "locomotive-scroll": "4.1.4",
        "lozad": "1.16.0",
        "ninelines-sharing": "1.5.8",
        "ninelines-ua-parser": "1.1.6",
        "normalize.css": "8.0.1",
        "object-fit-images": "3.2.4",
        "object-fit-videos": "1.0.4",
        "scroll-depth": "1.2.0",
        "gsap": "3.10.4",
        "lodash.debounce": "4.0.8",
        "lodash.throttle": "4.1.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.(js|jsx|ts|tsx)": "eslint",
        "*.(css|scss)": "stylelint",
        "*.pug": "pug-lint"
    }
}
